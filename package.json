{
  "name": "note-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --turbo",
    "dev:collab": "npx tsx server/collab-server.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "check:env": "node scripts/check-env.js",
    "vercel:env": "bash vercel-env-setup.sh",
    "vercel:env:win": "powershell -ExecutionPolicy Bypass -File vercel-env-setup.ps1",
    "db:migrate": "npx tsx scripts/migrate-database.ts",
    "db:migrate:create": "npx tsx scripts/migrate-database.ts --create-only",
    "db:generate": "prisma generate",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:push": "npx tsx scripts/push-database.ts",
    "db:reset": "npx tsx scripts/reset-database.ts",
    "db:reset:force": "npx tsx scripts/reset-database.ts --force",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:reset": "npx tsx prisma/seed.ts --reset",
    "db:test": "dotenv -e .env.local -- node scripts/test-db-connection.js",
    "db:health": "node scripts/check-database-health.js",
    "db:validate": "npx tsx scripts/validate-database.ts",
    "db:test:migrations": "npx tsx scripts/test-migrations.ts",
    "db:test:both-modes": "npx tsx scripts/test-both-modes.ts",
    "db:export": "npx tsx scripts/export-data.ts",
    "db:import": "npx tsx scripts/import-data.ts",
    "startup:validate": "npx tsx scripts/validate-startup.ts",
    "verify:migration": "npx tsx scripts/verify-migration.ts",
    "supabase:test": "dotenv -e .env.local -- node scripts/test-supabase-connection.js",
    "db:migrate:supabase": "npx tsx scripts/migrate-to-supabase.ts",
    "supabase:setup": "cp prisma/schema.supabase.prisma prisma/schema.prisma && npm run db:migrate -- --name init_supabase && npm run db:generate",
    "verify:sync": "dotenv -e .env -- node scripts/verify-sync-functionality.js",
    "verify:network": "powershell -ExecutionPolicy Bypass -File scripts/diagnose-connection.ps1",
    "verify:quick": "dotenv -e .env -- node scripts/quick-verify.js",
    "storage:create": "dotenv -e .env.local -- npx tsx scripts/create-storage-bucket.ts",
    "test:storage": "dotenv -e .env.local -- npx tsx scripts/test-storage-setup.ts",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "audit": "npx tsx scripts/audit/index.ts",
    "audit:full": "npx tsx scripts/audit/index.ts --full",
    "audit:category": "npx tsx scripts/audit/index.ts --category",
    "audit:components": "npx tsx scripts/audit/index.ts --components",
    "audit:performance": "npx tsx scripts/audit/index.ts --performance",
    "audit:a11y": "npx tsx scripts/audit/index.ts --accessibility",
    "audit:report": "npx tsx scripts/audit/index.ts --report-only"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hocuspocus/extension-database": "^3.4.1",
    "@hocuspocus/extension-logger": "^3.4.1",
    "@hocuspocus/provider": "^2.15.3",
    "@hocuspocus/server": "^3.4.0",
    "@hookform/resolvers": "^5.2.2",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.83.0",
    "@tiptap/extension-collaboration": "^2.27.1",
    "@tiptap/extension-collaboration-cursor": "^2.26.2",
    "@tiptap/extension-image": "^2.27.1",
    "@tiptap/extension-placeholder": "^2.27.1",
    "@tiptap/react": "^2.27.1",
    "@tiptap/starter-kit": "^2.27.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "es-abstract": "^1.24.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "katex": "^0.16.27",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.7",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.3",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "use-debounce": "^10.0.6",
    "yjs": "^13.6.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@fast-check/vitest": "^0.2.4",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.10",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "fast-check": "^4.4.0",
    "jsdom": "^27.2.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.10"
  }
}
